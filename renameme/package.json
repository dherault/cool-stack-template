{
  "name": "renameme",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "bunx --bun tsc && bunx eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "router": "bun src/router/logic/index.ts && bunx eslint ./src/router --fix",
    "extract-currencies": "bun ./scripts/extract-currencies",
    "analyze": "npx vite-bundle-visualizer",
    "emulators": "mkdir -p emulators-data/current-export && firebase emulators:start --only auth,functions,storage,firestore,extensions --import=./emulators-data/current-export --export-on-exit=./emulators-data/current-export",
    "deploy-rules": "firebase deploy --only firestore:rules,storage"
  },
  "dependencies": {
    "@dnd-kit/sortable": "^8.0.0",
    "@hookform/resolvers": "^3.6.0",
